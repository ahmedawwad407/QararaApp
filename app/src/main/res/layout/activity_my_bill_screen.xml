<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MyBillScreen"
    tools:ignore="MissingPrefix">

    <com.mikhaellopez.circularprogressbar.CircularProgressBar
        android:id="@+id/circularProgressBar6"
        android:layout_width="@dimen/_75sdp"
        android:layout_height="@dimen/_75sdp"
        app:cpb_background_progressbar_color="@color/white"
        app:cpb_background_progressbar_width="5dp"
        app:cpb_progress_direction="to_left"
        app:cpb_progressbar_color="@color/colorPrimary"
        app:cpb_progressbar_width="10dp"
        app:cpb_round_border="false"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/circularProgressBar5"
        app:layout_constraintStart_toStartOf="@+id/circularProgressBar5"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <TextView
        android:id="@+id/tx22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingRight="5dp"
        android:text="10"
        android:textColor="@color/twitter_blue"
        app:layout_constraintBottom_toBottomOf="@+id/circularProgressBar6"
        app:layout_constraintEnd_toEndOf="@+id/circularProgressBar6"
        app:layout_constraintStart_toStartOf="@+id/circularProgressBar6"
        app:layout_constraintTop_toTopOf="@+id/circularProgressBar6" />

    <com.mikhaellopez.circularprogressbar.CircularProgressBar
        android:id="@+id/circularProgressBar4"
        android:layout_width="@dimen/_75sdp"
        android:layout_height="@dimen/_75sdp"
        app:cpb_background_progressbar_color="@color/white"
        app:cpb_background_progressbar_width="5dp"
        app:cpb_progress_direction="to_left"
        app:cpb_progressbar_color="@color/colorPrimary"
        app:cpb_progressbar_width="10dp"
        app:cpb_round_border="false"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/circularProgressBar"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/circularProgressBar"
        app:layout_constraintTop_toTopOf="@+id/textView7" />

    <com.mikhaellopez.circularprogressbar.CircularProgressBar
        android:id="@+id/circularProgressBar5"
        android:layout_width="@dimen/_75sdp"
        android:layout_height="@dimen/_75sdp"
        app:cpb_background_progressbar_color="@color/white"
        app:cpb_background_progressbar_width="5dp"
        app:cpb_progress_direction="to_left"
        app:cpb_progressbar_color="@color/colorPrimary"
        app:cpb_progressbar_width="10dp"
        app:cpb_round_border="false"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="@+id/circularProgressBar4"
        app:layout_constraintStart_toStartOf="@+id/circularProgressBar4"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:theme="@style/AppTheme.AppBarOverlay"
        app:contentInsetStartWithNavigation="0dp"
        app:layout_collapseMode="pin"
        android:background="@drawable/rectangel_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_scrollFlags="scroll|enterAlways"
        app:popupTheme="@style/AppTheme.PopupOverlay"
        app:titleTextColor="@color/textColorPrimary"
        tools:ignore="DuplicateIds,MissingConstraints">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/ivBack"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/spacing_standard_new"
                android:backgroundTint="@color/gray_80"
                android:clickable="true"
                android:contentDescription="@string/text_todo"
                android:focusable="true"
                android:padding="@dimen/_5sdp"
                android:src="@drawable/ic_back"
                android:visibility="visible" />

            <TextView
                android:id="@+id/ivasad"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/ivBack"
                android:layout_toRightOf="@+id/ivBack"
                android:clickable="true"
                android:contentDescription="@string/text_todo"
                android:focusable="true"
                android:padding="@dimen/_5sdp"
                android:text="فاتورتي"
                android:textColor="#000"
                android:textSize="@dimen/_20sdp"
                android:textStyle="bold" />
        </RelativeLayout>
    </androidx.appcompat.widget.Toolbar>


    <TextView
        android:id="@+id/textView3"
        style="@style/Heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="204dp"
        android:layout_marginTop="40dp"
        android:text="رقم الاشتراك : "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/tvLblReview"
        style="@style/Heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="//////////"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3" />

    <TextView
        android:id="@+id/tvProductReviewCmt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="8dp"
        android:text="اسم المستخدم : "
        android:textAppearance="@style/FontMedium"
        android:textColor="@color/textColorPrimary"
        android:textSize="@dimen/font_size_small"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/Tx6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tvProductReviewDuration1"
        android:layout_marginTop="8dp"
        android:layout_toStartOf="@+id/tvProductReviewDuration1"
        android:text="//////////"
        android:textColor="@color/textColorSecondary"
        app:layout_constraintBottom_toBottomOf="@+id/tvProductReviewDuration1"
        app:layout_constraintEnd_toEndOf="@+id/Tx4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Tx4"
        app:layout_constraintTop_toBottomOf="@+id/Tx4" />


    <TextView
        android:id="@+id/Tx4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:layout_toStartOf="@+id/tvProductReviewCmt1"
        android:text="//////////"
        android:textAppearance="@style/FontMedium"
        android:textColor="@color/textColorPrimary"
        android:textSize="@dimen/font_size_small"
        app:layout_constraintEnd_toEndOf="@+id/tvLblReview"
        app:layout_constraintStart_toStartOf="@+id/tvLblReview"
        app:layout_constraintTop_toBottomOf="@+id/tvLblReview" />

    <TextView
        android:id="@+id/tvProductReviewDuration1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvProductReviewCmt1"
        android:layout_alignEnd="@+id/tvProductReviewCmt1"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:text="تاريخ الفاتورة : "
        android:textColor="@color/textColorSecondary"
        app:layout_constraintEnd_toEndOf="@+id/tvProductReviewCmt1"
        app:layout_constraintTop_toBottomOf="@+id/tvProductReviewCmt1" />

    <com.mikhaellopez.circularprogressbar.CircularProgressBar
        android:id="@+id/circularProgressBar"
        android:layout_width="@dimen/_75sdp"
        android:layout_height="@dimen/_75sdp"
        android:layout_marginStart="72dp"
        app:cpb_background_progressbar_color="@color/white"
        app:cpb_background_progressbar_width="5dp"
        app:cpb_progress_direction="to_left"
        app:cpb_progressbar_color="@color/colorPrimary"
        app:cpb_progressbar_width="10dp"
        app:cpb_round_border="false"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.25" />

    <TextView
        android:id="@+id/Tx14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingRight="5dp"
        android:text="50"
        android:textColor="@color/twitter_blue"
        app:layout_constraintBottom_toBottomOf="@+id/circularProgressBar4"
        app:layout_constraintEnd_toEndOf="@+id/circularProgressBar4"
        app:layout_constraintStart_toStartOf="@+id/circularProgressBar4"
        app:layout_constraintTop_toTopOf="@+id/circularProgressBar4" />

    <TextView
        android:id="@+id/Tx16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingRight="5dp"
        android:text="100"
        android:textColor="@color/twitter_blue"
        app:layout_constraintBottom_toBottomOf="@+id/circularProgressBar5"
        app:layout_constraintEnd_toEndOf="@+id/circularProgressBar5"
        app:layout_constraintStart_toStartOf="@+id/circularProgressBar5"
        app:layout_constraintTop_toTopOf="@+id/circularProgressBar5" />

    <TextView
        android:id="@+id/Tx8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingRight="5dp"
        android:text="100"
        android:textColor="@color/twitter_blue"
        app:layout_constraintBottom_toBottomOf="@+id/circularProgressBar"
        app:layout_constraintEnd_toEndOf="@+id/circularProgressBar"
        app:layout_constraintStart_toStartOf="@+id/circularProgressBar"
        app:layout_constraintTop_toTopOf="@+id/circularProgressBar" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginTop="@dimen/_50sdp"
        android:gravity="center"
        android:paddingRight="5dp"
        android:text="كمية الاستهلاك المياه"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/circularProgressBar"
        app:layout_constraintTop_toBottomOf="@+id/tvProductReviewDuration1" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:paddingRight="5dp"
        android:layout_marginTop="@dimen/_50sdp"
        android:text="صرف صحي"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginTop="@dimen/_50sdp"
        android:gravity="center"
        android:paddingRight="5dp"
        android:text="متأخرات"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />


    <TextView
        android:id="@+id/textView9"
        android:layout_width="150dp"
        android:layout_height="35dp"
        android:layout_marginTop="@dimen/_50sdp"
        android:gravity="center"
        android:paddingRight="5dp"
        android:text="إجمالي الفاتورة الحالية"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />


</androidx.constraintlayout.widget.ConstraintLayout>